<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.yingfu.line">

    <application
        android:name="line.LineApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="line.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="line.line.IndexLineActivity"/>
        <activity android:name="line.scroller.ScrollViewActivity"/>
        <activity android:name="line.bezier.BezierActivity"/>
        <activity android:name="line.tree.TreeActivity"/>
    </application>

</manifest>