<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.yingfu.line">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:name="line.LineApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity android:name="line.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="line.line.IndexLineActivity">
        </activity>
        <activity android:name="line.scroller.ScrollViewActivity"/>
        <activity android:name="line.bezier.BezierActivity"/>
        <activity android:name="line.javafoundation.tree.TreeActivity"/>
        <activity android:name="line.svg.SVGActivity"/>
        <activity android:name="line.viewpager.ViewPagerActivity">
        </activity>
        <activity android:name="line.flutter.MyFlutterActivity"/>
        <activity android:name="line.panorama.OpenGLActivity"/>
        <activity android:name="line.video360.VideoActivity"/>
        <activity android:name="line.panorama.GoogleVRActivity">
        </activity>
    </application>
    <uses-sdk tools:overrideLibrary="com.google.vr.widgets.pano,com.google.vr.widgets.common,com.google.vr.sdk.widgets.pano,com.google.vr.cardboard"/>
</manifest>