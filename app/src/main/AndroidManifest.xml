<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.yingfu.line">

    <uses-sdk tools:overrideLibrary="com.google.vr.widgets.pano,com.google.vr.widgets.common,com.google.vr.sdk.widgets.pano,com.google.vr.cardboard" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.line.base.LineApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:label">
        <activity
            android:name="com.line.compose.TestComposeActivity"
            android:exported="false" />


        <activity
            android:name="com.line.view.scroller.ScrollWebViewActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="com.line.view.anim.AnimActivity" />
        <activity android:name="com.line.view.bookpager.BookPageActivity" />
        <activity
            android:name="com.line.binder.MainAIDLActivity"
            android:process="com.example.yingfu.line.aidl" />

        <service android:name="com.line.binder.BinderPoolService">

        </service>

        <activity android:name="com.line.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.line.view.line.IndexLineActivity" />
        <activity android:name="com.line.view.scroller.ScrollViewActivity" />
        <activity android:name="com.line.view.bezier.BezierActivity" />
        <activity android:name="com.line.view.svg.SVGActivity" />
        <activity android:name="com.line.view.viewpager.ViewPagerActivity" /> <!-- <activity android:name="line.flutter.MyFlutterActivity" /> -->
        <activity android:name="com.line.view.viewpager.NestedViewPagerActivity" />
    </application>

</manifest>